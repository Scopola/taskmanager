<Project Sdk="Microsoft.NET.Sdk;Microsoft.NET.Sdk.Publish">
 <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
 <ItemGroup>
   <None Remove="Data\AssignedTaskSourceType.json" />
   <None Remove="Data\HpdUsages.json" />
   <None Remove="Data\ProductActionType.json" />
   <None Remove="Data\SourceCategories.json" />
   <None Remove="Data\TasksSeedData.json" />
   <None Remove="Data\Users.json" />
 </ItemGroup>
 <ItemGroup>
   <Content Include="Data\AssignedTaskType.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
   <Content Include="Data\HpdUsages.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
   <Content Include="Data\ProductActionType.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
   <Content Include="Data\SourceCategories.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
   <Content Include="Data\TasksSeedData.json">
     <CopyToOutputDirectory>Always</CopyToOutputDirectory>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
   </Content>
   <Content Include="Data\Users.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
   <Content Include="Data\Users.json">
     <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
 </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="9.0.0" />
    <PackageReference Include="Microsoft.Azure.Services.AppAuthentication" Version="1.3.1" />
    <PackageReference Include="Microsoft.Azure.WebJobs" Version="3.0.14" />
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions" Version="3.0.5" />
    <PackageReference Include="Microsoft.Azure.WebJobs.Logging" Version="3.0.14" />
